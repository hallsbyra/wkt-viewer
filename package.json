{
  "name": "wkt-viewer",
  "displayName": "WKT Viewer",
  "description": "A VS Code extension to view WKT geometries dynamically based on text selection.",
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "workspaces": [
    "webview"
  ],
  "contributes": {
    "commands": [
      {
        "command": "wkt-viewer.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "wktViewer.start",
        "title": "Start WKT Viewer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run -w webview build && npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.3.2",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "vite": "^6.3.4"
  },
  "dependencies": {
    "ol": "^10.5.0"
  }
}
