{
  "name": "wkt-viewer",
  "displayName": "WKT Viewer",
  "description": "A VS Code extension to visualize WKT (Well-Known Text) geometries in text files.",
  "keywords": [
    "vscode",
    "extension",
    "wkt",
    "viewer",
    "geometry",
    "visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hallsbyra/wkt-viewer"
  },
  "version": "0.0.1",
  "publisher": "halls-byra",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension/dist/extension.js",
  "workspaces": [
    "extension",
    "webview"
  ],
  "contributes": {
    "commands": [
      {
        "command": "wkt-viewer.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "wktViewer.start",
        "title": "Start WKT Viewer"
      }
    ]
  },
  "scripts": {
    "clean": "npm run clean --workspaces",
    "vscode:prepublish": "npm run clean && npm run -w webview build && npm run -w extension compile && eslint",
    "watch": "tsc --build --watch",
    "package": "vsce package",
    "test": "npm run test --workspaces"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/mocha": "^10.0.10",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.3.2",
    "eslint": "^9.26.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.32.0"
  },
  "dependencies": {
  }
}
